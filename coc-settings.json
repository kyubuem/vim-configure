{
	"coc.preferences.formatOnSaveFiletypes": [
		"dart",
		"golang"
	],
	"flutter.lsp.initialization.closingLabels": true,
	"clangd.path": "/usr/local/opt/llvm/bin/clangd",
	"languageserver": {
		"dart":{
			"command": "/Users/goodboy/dev/flutter/bin/cache/dart-sdk/bin/dart",
			"args": [
				"/Users/goodboy/dev/flutter/bin/cache/dart-sdk/bin/snapshots/analysis_server.dart.snapshot",
				"--lsp"
			],
			"filetypes": ["dart"],
			"disableDynamicRegister": true,
			"trace.server": "verbose"
